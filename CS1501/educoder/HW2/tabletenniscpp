#include <iostream>
#include <math.h>
using  namespace std;
int main()
{
    int a,b;
    cin>>a>>b;
    if(a<10&&b<10) cout<<"In progress"<<endl;
    else if(a==11&&b<10) cout<<"A win"<<endl;
    else if(b==11&&a<10) cout<<"B win"<<endl;
    else if(abs(a-b)==2&&a>b) cout<<"A win"<<endl;
    else if(abs(a-b)==2&&a<b) cout<<"B win"<<endl;
    else if(abs(a-b)<2) cout<<"In progress"<<endl;
    else cout<<"Illegal"<<endl; 
    return 0;
}